    "start": "turbo run start",
    "test:a2a": "turbo run test:a2a --filter=api",
    "test:a2a:routing": "turbo run test:a2a:routing --filter=api"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "turbo": "^1.13.4"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "openai": "^4.98.0",
    "uuid": "^11.1.0",
    "@nestjs/config": "^3.0.0"
  }
}
